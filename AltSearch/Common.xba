			end if

			pom(0).atr = 0  &apos;1 is for search of exact values
			&apos; zde vytvorit pole pro atributy
			attrnames() = ArrayOutOfString_(cmd, &quot;|&quot;)
			dim SrchAtrTmp(ubound(attrnames())) as new com.sun.star.beans.PropertyValue
